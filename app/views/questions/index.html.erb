<h2>Questions</h2>
<div class="row">
  <div class="col-md-12"> 
    <table class="table table-striped">
      <thead class="thead-light">
        <tr>
          <tr>ID</tr>
          <tr>Title</tr>
          <tr>Menu</tr>
        </tr>
      </thead>
      <tbody>
        <% @questions.each do |question| %>
          <tr>
            <td><%= question.id %></td>
            <td><%= question.title %></td>
            <td>[Edit][Delete]</td>
          </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>


